	public class Persistable {

		/*
		 * (non-javadoc)
		 */
		private string ID;

		/**
		 * Getter of the property <tt>ID</tt>
		 * 
		 * @return Returns the ID.
		 * 
		 */

		public string getID() {
			return ID;
		}

		/**
		 * Setter of the property <tt>ID</tt>
		 * 
		 * @param ID
		 *            The ID to set.
		 * 
		 */
		public void setID(string ID) {
			this.ID = ID;
		}

	}
